{
  "name": "myorg-feature-top",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/myorg/feature-top/src",
  "prefix": "myorg",
  "targets": {
    "build": {
      "executor": "@nx/angular:ng-packagr-lite",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/myorg/feature-top/ng-package.json",
        "tailwindConfig": "libs/myorg/feature-top/tailwind.config.js"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/myorg/feature-top/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/myorg/feature-top/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/myorg/feature-top/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/myorg/feature-top/**/*.ts",
          "libs/myorg/feature-top/**/*.html"
        ]
      }
    },
    "component-test": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "libs/myorg/feature-top/cypress.config.ts",
        "testingType": "component",
        "skipServe": true,
        "devServerTarget": "myorg:build"
      }
    }
  },
  "tags": []
}
